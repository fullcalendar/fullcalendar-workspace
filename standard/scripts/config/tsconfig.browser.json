{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2015",
    "moduleResolution": "node16",
    "allowJs": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "noUnusedLocals": true,
    "noImplicitUseStrict": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "createElement",
    "strictBindCallApply": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "types": [],
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true
  }
}

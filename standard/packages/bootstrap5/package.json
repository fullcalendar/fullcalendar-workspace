{
  "name": "@fullcalendar/bootstrap5",
  "version": "7.0.0-beta.1",
  "title": "FullCalendar Bootstrap 5 Plugin",
  "description": "Bootstrap 5 theme for FullCalendar",
  "keywords": [
    "bootstrap",
    "bootstrap5"
  ],
  "homepage": "https://fullcalendar.io/docs/bootstrap5",
  "peerDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-beta.1",
    "bootstrap": ">=5.2.0 <6.0.0"
  },
  "devDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-beta.1",
    "@fullcalendar-scripts/standard": "workspace:*",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.11.3"
  },
  "scripts": {
    "build": "standard-scripts pkg:build",
    "clean": "standard-scripts pkg:clean",
    "lint": "eslint ."
  },
  "type": "module",
  "tsConfig": {
    "extends": "@fullcalendar-scripts/standard/config/tsconfig.browser.json",
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "./dist/.tsout"
    },
    "include": [
      "./src/**/*"
    ]
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": true
      },
      "./internal": {}
    },
    "iifeGlobals": {
      ".": "FullCalendar.Bootstrap5",
      "./internal": "FullCalendar.Bootstrap5.Internal"
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}

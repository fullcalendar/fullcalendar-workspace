    "@fullcalendar/adaptive": "workspace:7.0.0-beta.5",
    "@fullcalendar/scrollgrid": "workspace:7.0.0-beta.5",
    "@fullcalendar/list": "workspace:7.0.0-beta.5",
    "@fullcalendar/multimonth": "workspace:7.0.0-beta.5",

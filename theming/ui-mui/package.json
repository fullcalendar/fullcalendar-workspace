    "@fullcalendar/scrollgrid": { "optional": true },
    "@fullcalendar/list": { "optional": true },
    "@fullcalendar/multimonth": { "optional": true }
  },
  "devDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-beta.5",
    "@fullcalendar/daygrid": "workspace:7.0.0-beta.5",
    "@fullcalendar/timegrid": "workspace:7.0.0-beta.5",
    "@fullcalendar/interaction": "workspace:7.0.0-beta.5",
    "@fullcalendar/timeline": "workspace:7.0.0-beta.5",
    "@fullcalendar/resource-daygrid": "workspace:7.0.0-beta.5",
    "@fullcalendar/resource-timegrid": "workspace:7.0.0-beta.5",
    "@fullcalendar/resource-timeline": "workspace:7.0.0-beta.5",
    "@fullcalendar/adaptive": "workspace:7.0.0-beta.5",
    "@fullcalendar/scrollgrid": "workspace:7.0.0-beta.5",
    "@fullcalendar/list": "workspace:7.0.0-beta.5",
    "@fullcalendar/multimonth": "workspace:7.0.0-beta.5",
    "@fullcalendar/react": "workspace:*",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "scripts": {
    "build": "standard-scripts pkg:build && pnpm run clean-dist",
    "clean": "standard-scripts pkg:clean",
    "lint": "eslint .",
    "clean-dist": "rm -f dist/*/global.js dist/*/global.min.js dist/*/global.css.js"
  },
  "type": "module",
  "tsConfig": {
    "compilerOptions": {
      "target": "ES2020",
      "useDefineForClassFields": true,
      "module": "node16",
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "skipLibCheck": true,
      "jsx": "react",

      "moduleResolution": "node16",
      "isolatedModules": true,
      "moduleDetection": "force",

      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,

      "composite": true,
      "declaration": true,
      "declarationMap": true,
      "rootDir": "./src",
      "outDir": "./dist/.tsout"
    },
    "include": [
      "./src/**/*"
    ]
  },
  "buildConfig": {
    "exports": {
      "./breezy/global": { "iife": true, "css": true },
      "./breezy/EventCalendar": { "module": true },
      "./breezy/EventCalendarView": { "module": true },
      "./breezy/EventCalendarToolbar": { "module": true },
      "./breezy/Scheduler": { "module": true },
      "./breezy/SchedulerView": { "module": true },
      "./classic/global": { "iife": true, "css": true },
      "./classic/EventCalendar": { "module": true },
      "./classic/EventCalendarView": { "module": true },
      "./classic/EventCalendarToolbar": { "module": true },
      "./classic/Scheduler": { "module": true },
      "./classic/SchedulerView": { "module": true },
      "./forma/global": { "iife": true, "css": true },
      "./forma/EventCalendar": { "module": true },
      "./forma/EventCalendarView": { "module": true },
      "./forma/EventCalendarToolbar": { "module": true },
      "./forma/Scheduler": { "module": true },
      "./forma/SchedulerView": { "module": true },
      "./monarch/global": { "iife": true, "css": true },
      "./monarch/EventCalendar": { "module": true },
      "./monarch/EventCalendarView": { "module": true },
      "./monarch/EventCalendarToolbar": { "module": true },
      "./monarch/Scheduler": { "module": true },
      "./monarch/SchedulerView": { "module": true },
      "./pulse/global": { "iife": true, "css": true },
      "./pulse/EventCalendar": { "module": true },
      "./pulse/EventCalendarView": { "module": true },
      "./pulse/EventCalendarToolbar": { "module": true },
      "./pulse/Scheduler": { "module": true },
      "./pulse/SchedulerView": { "module": true }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}

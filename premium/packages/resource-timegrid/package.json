{
  "name": "@fullcalendar/resource-timegrid",
  "version": "7.0.0-rc.0",
  "title": "FullCalendar Resource Time Grid Plugin",
  "description": "Displays events on a vertical resource view with time slots",
  "keywords": [
    "scheduler",
    "resource"
  ],
  "homepage": "https://fullcalendar.io/docs/vertical-resource-view",
  "dependencies": {
    "@fullcalendar/daygrid": "workspace:7.0.0-rc.0",
    "@fullcalendar/premium-common": "workspace:7.0.0-rc.0",
    "@fullcalendar/resource-daygrid": "workspace:7.0.0-rc.0",
    "@fullcalendar/timegrid": "workspace:7.0.0-rc.0"
  },
  "peerDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-rc.0",
    "@fullcalendar/resource": "workspace:7.0.0-rc.0"
  },
  "devDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-rc.0",
    "@fullcalendar/resource": "workspace:7.0.0-rc.0",
    "@fullcalendar-scripts/standard": "workspace:*"
  },
  "scripts": {
    "build": "standard-scripts pkg:build",
    "clean": "standard-scripts pkg:clean",
    "lint": "eslint ."
  },
  "type": "module",
  "tsConfig": {
    "extends": "@fullcalendar-scripts/standard/config/tsconfig.browser.json",
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "./dist/.tsout"
    },
    "include": [
      "./src/**/*"
    ]
  },
  "buildConfig": {
    "exports": {
      ".": {
        "module": true,
        "globalVar": "FullCalendar.ResourceTimeGrid"
      },
      "./global": {
        "iife": true,
        "globalVar": "FullCalendar.ResourceTimeGrid"
      },
      "./internal": {
        "module": true,
        "globalVar": "FullCalendar.ResourceTimeGrid.Internal"
      }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}

{
  "name": "@fullcalendar/resource",
  "version": "7.0.0-rc.0",
  "title": "FullCalendar Resource Plugin",
  "description": "Base support for resources, required by resource views",
  "keywords": [
    "scheduler",
    "resource"
  ],
  "dependencies": {
    "@fullcalendar/premium-common": "workspace:7.0.0-rc.0"
  },
  "peerDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-rc.0"
  },
  "devDependencies": {
    "@fullcalendar/core": "workspace:7.0.0-rc.0",
    "@fullcalendar-scripts/standard": "workspace:*"
  },
  "scripts": {
    "build": "standard-scripts pkg:build",
    "clean": "standard-scripts pkg:clean",
    "lint": "eslint ."
  },
  "type": "module",
  "tsConfig": {
    "extends": "@fullcalendar-scripts/standard/config/tsconfig.browser.json",
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "./dist/.tsout"
    },
    "include": [
      "./src/**/*"
    ]
  },
  "buildConfig": {
    "exports": {
      ".": {
        "module": true,
        "globalVar": "FullCalendar.Resource"
      },
      "./global": {
        "iife": true,
        "globalVar": "FullCalendar.Resource"
      },
      "./internal": {
        "module": true,
        "globalVar": "FullCalendar.Resource.Internal"
      }
    }
  },
  "sideEffects": true,
  "sideEffects#comment": [
    "Rollup incorrectly treeshakes the registerResourceSourceDef system"
  ],
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
